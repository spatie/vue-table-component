{
    "presets": [
        ["babel-preset-env", {
            "targets": {
                "browsers": [
                    ">0.25%",
                    "not ie 11",
                    "not op_mini all"
                ],
                "uglify": true
            },
            "modules": false
        }]
    ],
    "plugins": [
        "babel-plugin-transform-vue-jsx",
        "babel-plugin-transform-object-rest-spread"
    ],
    "env": {
        "test": {
            "presets": [
                ["env", {
                    "targets": {
                        "node": "current"
                    }
                }]
            ]
        }
    }
}
